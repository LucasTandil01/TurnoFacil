	@Override
	public int getAnio() {return anio;}

	@Override
	public ArrayList<Elemento> buscar(Filtro f) {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	public int cantVehiculos() {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public Elemento getCopia(Filtro f) {
		// TODO Auto-generated method stub
		return null;
	}
	
}
