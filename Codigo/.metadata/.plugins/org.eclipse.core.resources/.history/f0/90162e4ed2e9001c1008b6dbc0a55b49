
public class Usuario {
	private int dni;
	private String nombre;
	
	Usuario(int dni, String nombre){
		this.dni = dni;
		this.nombre = nombre;
	}	
	

	@Override
	public boolean equals(Object o) {
		if(dni.equals())
	}
	
	public int getDni() {
		return dni;
	}

	public void setDni(int dni) {
		this.dni = dni;
	}

	public String getNombre() {
		return nombre;
	}

	public void setNombre(String nombre) {
		this.nombre = nombre;
	}
}
